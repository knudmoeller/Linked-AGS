@prefix : <http://example.com/schema/data_conversion#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ags: <http://datalysator.com/lod/ags/schema/> .

<#> a :Spec ; 
	:format :CSV ;
	:header_rows 5 ;
	:resource <#geo_entity> ;
	:base_uri "http://datalysator.com/lod/ags/" ;
	:null_values ("" "NULL");
.

<#geo_entity> a :Resource ;
    :identity [ 
        :source_columns (3 4 5 6 7) ;
	 	:source_column_glue "/" ;
    ] ;
    :attribute [
        :property rdfs:label ; 
        :source_column 8 ;
    ] ;
    :attribute [
        :property ags:ags ; 
        :source_columns (3 4 5 7) ;
		:source_column_glue "" ;
	] ;
    :attribute [
        :property ags:ars ; 
        :source_columns (3 4 5 6 7) ;
		:source_column_glue "" ;
	] ;
	:attribute [
		:property ags:satzart ;
		:source_column 1 ;
		:lookup <#satz_lookup> ;
	] ;
	:relationship [
		:property rdf:type ;
		:source_column 2;
		:lookup <#type_lookup> ;
	] ;
.

<#satz_lookup> a :Lookup ;
	:lookup_entry [ :lookup_key "10"; :lookup_value ags:Land ] ;
	:lookup_entry [ :lookup_key "20"; :lookup_value ags:Regierungsbezirk ] ;
	:lookup_entry [ :lookup_key "30"; :lookup_value ags:Region ] ;
	:lookup_entry [ :lookup_key "40"; :lookup_value ags:Kreis ] ;
	:lookup_entry [ :lookup_key "50"; :lookup_value ags:Gemeindeverband ] ;
	:lookup_entry [ :lookup_key "60"; :lookup_value ags:Gemeinde ] ;
.

<#type_lookup> a :Lookup ;
	:lookup_entry [ :lookup_key "41"; :lookup_value ags:Kreisfreie_Stadt ] ;
	:lookup_entry [ :lookup_key "42"; :lookup_value ags:Stadtkreis ] ;
	:lookup_entry [ :lookup_key "43"; :lookup_value ags:Kreis ] ;
	:lookup_entry [ :lookup_key "44"; :lookup_value ags:Landkreis ] ;
	:lookup_entry [ :lookup_key "45"; :lookup_value ags:Regionalverband ] ;
	:lookup_entry [ :lookup_key "50"; :lookup_value ags:Verbandsfreie_Gemeinde ] ;
	:lookup_entry [ :lookup_key "51"; :lookup_value ags:Amt ] ;
	:lookup_entry [ :lookup_key "52"; :lookup_value ags:Samtgemeinde ] ;
	:lookup_entry [ :lookup_key "53"; :lookup_value ags:Verbandsgemeinde ] ;
	:lookup_entry [ :lookup_key "54"; :lookup_value ags:Verwaltungsgemeinschaft  ] ;
	:lookup_entry [ :lookup_key "55"; :lookup_value ags:Kirchspielslandgemeinde ] ;
	:lookup_entry [ :lookup_key "56"; :lookup_value ags:Verwaltungsverband  ] ;
	:lookup_entry [ :lookup_key "57"; :lookup_value ags:VG_Traegermodell ] ;
	:lookup_entry [ :lookup_key "58"; :lookup_value ags:Erfuellende_Gemeinde ] ;
	:lookup_entry [ :lookup_key "60"; :lookup_value ags:Markt ] ;
	:lookup_entry [ :lookup_key "61"; :lookup_value ags:Kreisfreie_Stadt ] ;
	:lookup_entry [ :lookup_key "62"; :lookup_value ags:Stadtkreis ] ;
	:lookup_entry [ :lookup_key "63"; :lookup_value ags:Stadt ] ;
	:lookup_entry [ :lookup_key "64"; :lookup_value ags:Kreisangehoerige_Gemeinde ] ;
	:lookup_entry [ :lookup_key "65"; :lookup_value ags:Gemeindefreies_Gebiet-bewohnt ] ;
	:lookup_entry [ :lookup_key "66"; :lookup_value ags:Gemeindefreies_Gebiet-unbewohnt ] ;
	:lookup_entry [ :lookup_key "67"; :lookup_value ags:Grosse_Kreisstadt ] ;
.
